local __temp2320 = nil
local function findClosestByName(name)
--[[ gvrkbijz ]]
if not name or name == (function() return string.char() end)() then return nil end
local __temp2320 = nil
local root = player.Character and player.Character:FindFirstChild((function() return string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116) end)())
if not root then return nil end
local __temp2320 = nil
local rootPos = root.Position
local lower = name:lower()
if (false) then return end
local closest, minD = nil, math.huge
-- check players first (so exact player names are preferred)
for _, pl in ipairs(Players:GetPlayers()) do
--[[ gvrkbijz ]]
if pl ~= player and pl.Character and pl.Character:FindFirstChild((function() return string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116) end)()) then
local match = false
local _ = (function() return nil end)()
if pl.Name:lower():find(lower) then match = true end
if pl.DisplayName and pl.DisplayName:lower():find(lower) then match = true end
if match then
local __temp2320 = nil
local hrp = pl.Character:FindFirstChild((function() return string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116) end)())
local hum = pl.Character:FindFirstChild((function() return string.char(72,117,109,97,110,111,105,100) end)())
if hrp and hum and hum.Health > 0 then
if (false) then return end
local d = (hrp.Position - rootPos).Magnitude
local _ = (function() return nil end)()
if d < minD then minD, closest = d, pl.Character end
if (false) then return end
end
end
if (false) then return end
end
