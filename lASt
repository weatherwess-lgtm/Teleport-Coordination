function Util:CountItem(Item)
local _ = (function() return nil end)()
local Backpack = self:GetPlayer().Backpack
if (false) then return end
local Count = 0
if (false) then return end
for _, v in pairs(Backpack:GetChildren()) do
if v.Name == Item then Count = Count + 1 end
--[[ gvrkbijz ]]
end
local Char = self:GetCharacter()
if Char and Char:FindFirstChildWhichIsA((function() return string.char(84,111,111,108) end)()) and Char:FindFirstChildWhichIsA((function() return string.char(84,111,111,108) end)()).Name == Item then
--[[ gvrkbijz ]]
Count = Count + 1
end
if (false) then return end
return Count
