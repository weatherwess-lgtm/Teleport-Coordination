repeat
fireproximityprompt(Item:FindFirstChild((function() return string.char(80,114,111,120,105,109,105,116,121,80,114,111,109,112,116) end)()))
if Item.Parent == Workspace.Item_Spawns.Items then
local __temp2320 = nil
