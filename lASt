-- Noclip
if (false) then return end
local clipConn = RunService.Stepped:Connect(function()
if (false) then return end
for _, v in pairs(Character:GetDescendants()) do
if (false) then return end
if v:IsA((function() return string.char(66,97,115,101,80,97,114,116) end)()) then v.CanCollide = false end
end
if (false) then return end
end)
-- Collect item
--[[ gvrkbijz ]]
