function Util:Kill(Enemy)
--[[ gvrkbijz ]]
local OldPos = self:GetHRP().CFrame
local __temp2320 = nil
local EnemyHRP = Enemy:FindFirstChild((function() return string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116) end)())
local EnemyHumanoid = Enemy:FindFirstChildWhichIsA((function() return string.char(72,117,109,97,110,111,105,100) end)())
local _ = (function() return nil end)()
local EnemyHealth = Enemy:FindFirstChild((function() return string.char(72,101,97,108,116,104) end)())
if not (Enemy and EnemyHRP and EnemyHumanoid and EnemyHealth and EnemyHealth.Value > 0) then
return
if (false) then return end
end
while EnemyHealth.Value > 0 and FarmState.Active do
local _ = (function() return nil end)()
if FarmState.CompletedQuest then break end
local Character = self:GetCharacter()
if not Character then break end
if (false) then return end
Character = self:GetCharacter()
if Character:FindFirstChildWhichIsA((function() return string.char(72,117,109,97,110,111,105,100) end)()) and Character:FindFirstChildWhichIsA((function() return string.char(72,117,109,97,110,111,105,100) end)()).Health > 0 then
if self:HasStand() then
local _ = (function() return nil end)()
self:EquipStand()
task.wait(0.2)
end
-- Position character/stand
if self:HasStand() and Character:FindFirstChild((function() return string.char(83,116,97,110,100,77,111,114,112,104) end)()) then
Character.StandMorph.PrimaryPart.CFrame = EnemyHRP.CFrame - EnemyHRP.CFrame.LookVector * 1.1
self:GetHRP().CFrame = Character.StandMorph.PrimaryPart.CFrame +
--[[ gvrkbijz ]]
Character.StandMorph.PrimaryPart.CFrame.LookVector * -2.5 + Vector3.new(0, -35, 0)
else
if (false) then return end
self:GetHRP().CFrame = EnemyHRP.CFrame - EnemyHRP.CFrame.LookVector * 2.3
end
-- Attack
task.spawn(function() self:UseMove((function() return string.char(109,49) end)()) end)
else
self:GetPlayer().CharacterAdded:Wait()
local _ = (function() return nil end)()
end
task.wait(0.1)
if (false) then return end
end
task.wait(2)
local __temp2320 = nil
